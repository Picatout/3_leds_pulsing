                      00008 ;==========================================================================
                      00009 
                      00152         LIST
                      00016     
0FFF   0FFB           00017     __config  _WDTE_OFF
                      00018     
                      00019     udata
0000                  00020 cntr res 1
                      00021  
                      00022     org 0
                      00023     ; set GP0,GP1,GP2 as output
0000   0C00           00024     movlw 0
0001   0006           00025     tris GPIO
                      00026     ; set TMR0 with 1:4 pre-scale
0002   0C82           00027     movlw 2|1<<7
0003   0002           00028     option
                      00029     
0004                  00030 loop
0004   02??           00031     incf cntr,F
0005   02??           00032     movfw cntr
                      00033     ; output binary count to GPIO
0006   0026           00034     movwf GPIO
                      00035     ; delay ~ 1msec
0007   0201           00036     movfw TMR0
0008   0743           00037     skpz
0009   0A??           00038     goto $-2
000A   0A??           00039     goto loop
                      00040     
0FFB                  00041     end
